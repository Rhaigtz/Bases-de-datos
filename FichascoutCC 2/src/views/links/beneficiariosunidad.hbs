<div class="container p-4">
    <div class="col">
        <div class="col">
            <div class="card text-center">
                <div class="card-body">
                    <div class="container">
                        <div class="row">
                            <div class="col"><div class="h3">Rut</div></div>
                            <div class="col"><div class="h3">Nombre</div></div>
                            <div class="col"><div class="h3">Apellido</div></div>
                            <div class="col"><div class="h3">Ciudad</div></div>
                            <div class="col"><div class="h3">Poblacion</div></div>
                            <div class="col"><div class="h3">Calle</div></div>
                            <div class="col"><div class="h3">Numero</div></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        {{#each links}}
        <div class="col">
            <div class="card text-center">
                <div class="card-body">
                    <div class="container">
                        <div class="row">
                            <div class="col">{{rut}}</div>
                            <div class="col">{{nombre}}</div>
                            <div class="col">{{apellido}}</div>
                            <div class="col">{{ciudad}}</div>
                            <div class="col">{{poblacion}}</div>
                            <div class="col">{{calle}}</div>
                            <div class="col">{{numero_c}}</div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        {{else}}
        <div class="col mx-auto">
            <div class="card card-body text-center">
                <h2>No Eres Dirigente....</h2>
                <img src="/img/malandrin.png" alt="Logo Unidad" width="786" height="353"  />
                <a href="/links">Volver</a>
            </div>
        </div>
        {{/each}}
    </div>
</div>